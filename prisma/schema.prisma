datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["jsonProtocol"]
}

model Movie {
  id          Int     @id @default(autoincrement()) @db.UnsignedInt
  title       String  @db.VarChar(50)
  description String? @db.Text
}
